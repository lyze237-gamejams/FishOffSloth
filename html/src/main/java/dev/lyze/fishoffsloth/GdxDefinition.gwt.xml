<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <source path="" />
	<inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
	<inherits name="Box2DLights" />
	<inherits name="com.badlogic.gdx.controllers" />
	<inherits name="com.badlogic.gdx.controllers.controllers-gwt" />
	<inherits name="com.badlogic.gdx.physics.box2d.box2d-gwt" />
	<inherits name="com.dongbat.jbump" />
	<inherits name="dev.lyze.fishoffsloth.Main" />
	<inherits name="libgdx_screenmanager" />
	<inherits name="libgdx_screenmanager_gwt" />
	<inherits name="regexodus" />
	<inherits name="space.earlygrey.shapedrawer" />
	<inherits name="com.gempukku.libgdx.lib"/>
	<inherits name="textratypist" />
    <entry-point class="dev.lyze.fishoffsloth.gwt.GwtLauncher" />
    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.map" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.utils.Direction" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.utils.Sloth" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.level.entities.Entity" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.gamepads.VirtualGamepad" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.level.LightWorldEntry" />
	<extend-configuration-property name="gdx.reflect.include" value="dev.lyze.fishoffsloth.level.players.Player" />
    <!-- These two lines reduce the work GWT has to do during compilation and also shrink output size. -->
    <set-property name="user.agent" value="gecko1_8, safari"/>
    <collapse-property name="user.agent" values="*" />
    <!-- Remove the "user.agent" lines above if you encounter issues with Safari or other Gecko browsers. -->
</module>